{
  "name": "reactlisting",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://vishnucramesh.github.io/react-redux-productListing",
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build:css": "postcss src/index.tailwind.css -o src/index.css",
    "watch:css": "postcss src/index.tailwind.css -o src/index.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.4.6",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "postcss-cli": "^6.1.1",
    "redux-mock-store": "^1.5.3",
    "tailwindcss": "^0.7.3"
  }
}
